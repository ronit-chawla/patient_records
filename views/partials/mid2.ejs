  <div class="row">
    <div class="column">
      <% if(error && error.length>0){ %>
      <div class="ui error message" role="alert">
        <%=error%>
      </div>
      <% } %>
      <% if(success && success.length>0){ %>
      <div class="ui success message" role="alert">
        <%=success%>
      </div>
      <% } %>
      <% if(warning && warning.length>0){%>
      <div class="ui warning message" role="alert">
        <%=warning%>
      </div>
      <% }%>
      <% if(same && same.length>0){%>
      <div class="ui warning message" role="alert">
        <%=same%>
        <div>
          <a href="<%=`/patients/${patient._id}`%>" class="positive ui button">Keep Both</a>
          <form action="<%=`/patients/${patient._id}?_method=DELETE`%>" method="POST" class="inline">
            <button class="ui negative button">Delete New Patient Created</button>
          </form>
        </div>
      </div>
      <% }%>
    </div>
  </div>
